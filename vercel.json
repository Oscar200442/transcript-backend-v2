{
  "rewrites": [{ "source": "/(.*)", "destination": "/" }]
  "version": 2,
  "builds": [
    {
      "src": "api/transcripts.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/transcripts",
      "dest": "/api/transcripts.js",
      "methods": ["POST", "GET"]
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/transcripts.js",
      "methods": ["POST", "GET"]
    }
  ]
}
